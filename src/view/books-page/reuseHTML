<script>
let books = document.querySelectorAll(".book")
console.log(books[0])
let data = [];
for(let bookHtml of books) {
  let id = bookHtml.parentNode.dataset.bookId;
  let titleAndAuthor = bookHtml.outerText.split("\n");
  let author = titleAndAuthor[0]
  let title = titleAndAuthor[1]
  console.log(`${id}, "${title}", "${author}"`);
}
</script>